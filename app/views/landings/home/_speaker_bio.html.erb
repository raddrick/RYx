            <h3 class="logo-font"><%= name %></h3>
            <ul class="social-links circle animated-effect-1 text-center">
              <% unless facebook.blank? %>
              <li class="facebook"><a target="_blank" href="https://www.facebook.com/<%= facebook %>"><i class="fa fa-facebook"></i></a></li>
              <% end %>
              <% unless twitter.blank? %>
              <li class="twitter"><a target="_blank" href="https://www.twitter.com/<%= twitter %>"><i class="fa fa-twitter"></i></a></li>
              <% end %>
              <% unless linkedin.blank? %>
              <li class="linkedin"><a target="_blank" href="https://www.linkedin.com/in/<%= linkedin %>"><i class="fa fa-linkedin"></i></a></li>
              <% end %>
              <% unless instagram.blank? %>
              <li class="instagram"><a target="_blank" href="https://www.instagram.com/<%= instagram %>"><i class="fa fa-instagram"></i></a></li>
              <% end %>
            </ul>
            <div class="separator-<%= config == 1 ? 2 : 3 %> visible-lg"></div>
              <p><%= bio %></p>
            <div class="separator-<%= config == 1 ? 3 : 2 %> visible-lg"></div>
