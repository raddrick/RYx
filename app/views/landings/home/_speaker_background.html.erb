          <% if (config == 1) %>
            <%= image_tag image_path("#{speaker}.jpg"), alt:"", height:"500px" %>
          <% else %>
            <%= image_tag image_path("#{speaker}.jpg"), alt:"", class:"to-right-block", height:"500px" %>
          <% end %>
          <div class="full-image-overlay text-center">
            <h3><%= title %></h3>
            <p><%= description %></p>
            <ul class="social-links circle animated-effect-1 text-center">
              <li class="facebook"><a target="_blank" href="http://www.facebook.com"><i class="fa fa-facebook"></i></a></li>
              <li class="twitter"><a target="_blank" href="http://www.twitter.com"><i class="fa fa-twitter"></i></a></li>
              <li class="googleplus"><a target="_blank" href="http://plus.google.com"><i class="fa fa-google-plus"></i></a></li>
            </ul>
            <iframe width="560" height="315" src="https://www.youtube.com/embed/<%= stub %>" frameborder="0" allowfullscreen></iframe>
          </div>
